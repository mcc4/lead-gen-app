generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url = env("DATABASE_URL")
  relationMode = "prisma"
}

model User {
  id        String     @id @default(cuid())
  name      String
  email     String     @unique
  leads     Lead[]
}

model Lead {
  id          String   @id @default(cuid())
  name        String
  email       String
  phone       String
  companyName String
  createdAt   DateTime @default(now())
  User        User?     @relation(fields: [userId], references: [id])
  userId      String?
  @@index([userId])
}